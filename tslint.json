{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "jsx-wrap-multiline": false,
    "semicolon": [true, "never"],
    "jsx-curly-spacing": false,
    "quotemark": [true, "single", "jsx-double"],
    "no-implicit-dependencies": [true, "dev"],
    "trailing-comma": [true, "never"]
  }
}
